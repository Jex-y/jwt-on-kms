{
  "name": "jwt-on-kms",
  "version": "1.1.6",
  "description": "Create JWT tokens with a KMS key that can be verified without hitting the KMS API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "format": "prettier --write index.ts tests/*.ts",
    "lint": "tslint -p tsconfig.json",
    "build": "rimraf ./dist & tsc ",
    "minify": "uglifyjs ./dist/index.js -o ./dist/index.min.js -c -m",
    "pretest": "npm run lint"
  },
  "author": "Edward Jex",
  "license": "ISC",
  "git": "https://github.com/Jex-y/jwt-on-kms",
  "keywords": [
    "jwt",
    "kms",
    "aws"
  ],
  "dependencies": {
    "@aws-sdk/client-kms": "^3.137.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "aws-sdk-client-mock": "^1.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4",
    "uglify-js": "^3.17.0"
  }
}
